[webservers]
# Production servers
server1.example.com ansible_user=deploy ansible_ssh_private_key_file=~/.ssh/deploy_key
server2.example.com ansible_user=deploy ansible_ssh_private_key_file=~/.ssh/deploy_key

# Staging servers
staging.example.com ansible_user=deploy ansible_ssh_private_key_file=~/.ssh/deploy_key

[all:vars]
# Common variables for all hosts
ansible_python_interpreter=/usr/bin/python3
docker_compose_version=1.29.2
